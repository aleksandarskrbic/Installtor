package configurator.model.parameter;

public class TextParameter extends AbstractParameter {

	private static final long serialVersionUID = -2108315345602989650L;

	private String value;
	
	public TextParameter(String name) {
		super(ParameterType.TEXT, name);
	}

	@Override
	public void setValue(Object value) {
		this.value = (String) value;
	}

	@Override
	public String getValue() {
		return value;
	}
}
